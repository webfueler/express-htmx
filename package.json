{
  "name": "@fnz-infra/registry",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build:server && concurrently npm:start:*",
    "start:dev": "webpack --watch",
    "start:css": "tailwindcss -i ./src/index.css -o ./public/assets/index.css --watch",
    "start:dev:watch": "nodemon --watch dist --watch src dist/index.js",
    "build": "concurrently npm:build:*",
    "build:server": "NODE_ENV=production webpack",
    "build:css": "tailwindcss -i ./src/input.css -o ./public/assets/index.css",
    "serve": "node dist/server/index.js",
    "lint": "eslint src --ext ts,tsx"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-ts-important-stuff": "^1.1.0",
    "eslint-config-ts-react-important-stuff": "^3.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "ts-loader": "^9.4.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@types/cors": "2.8.14",
    "@types/webpack-node-externals": "^3.0.0",
    "axios": "^1.5.0",
    "concurrently": "^8.2.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "nodemon": "^3.0.1",
    "tailwindcss": "^3.3.3",
    "typed-html": "^3.0.1",
    "webpack-node-externals": "^3.0.0",
    "zod": "^3.22.2"
  },
  "engines": {
    "node": ">=18"
  }
}
